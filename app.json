{
  "expo": {
    "name": "IETians Diary",
    "slug": "ietians-diary",
    "description": "Soul of Institute of Engineering and Technology - DAVV",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/logo-with-bg.png",
    "splash": {
      "image": "./assets/logo-with-bg.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-with-bg.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "app uses photo uploading for avatar creation"
        }
      ]
    ]
  }
}
